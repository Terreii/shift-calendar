{
  "version": 2,
  "name": "schichtkalender-rt",
  "alias": [
    "schichtkalender-rt.now.sh"
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/manifest.webmanifest",
      "dest": "/manifest.webmanifest"
    },
    {
      "src": "^/(.*).txt",
      "headers": {
        "content-type": "text/plain; charset=\"utf-8\""
      },
      "dest": "/$1.txt"
    },
    {
      "src": "^/sw.js",
      "headers": {
        "cache-control": "s-maxage=0"
      },
      "dest": "/sw.js"
    },
    {
      "src": "^/workbox(.*).js",
      "headers": {
        "cache-control": "s-maxage=0"
      },
      "dest": "/workbox$1.js"
    },
    {
      "src": "^/(.*).css",
      "headers": {
        "content-type": "text/css; charset=\"utf-8\""
      },
      "dest": "/$1.css"
    },
    {
      "src": "^/(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
}
